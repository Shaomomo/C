#include <unistd.h>
void my_putchar(char c)
{
  write(1, &c, 1);
}
void my_trspos_tab(int c, int l, char **tab)
{
  int compt1;
  int compt2;

  compt1 = 0;
  compt2 = 0;
  while (compt1 < c)
    {
      while(compt2 < l)
	{
	  my_putchar(tab[compt2][compt1]);
	  compt2 = compt2 + 1;
	}
      // my_putchar(tab[compt2][compt1]);
      my_putchar('\n');
      compt1 = compt1 + 1;
      compt2 = 0;
    }
}
int main(void)
{
  char *arr[] = {"abcdefghij",
		 "0123456789",
		 "qwertyuiop"};
  my_trspos_tab(10, 3, arr);
  return (0);
}
