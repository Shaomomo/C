void	 my_putchar(char c);
int	 my_strlen( char *str)
{
  int	 i;
  int	 compt;

  i = 0;
  compt = 1;
  while (str[i] != '\0')
    {
      str[i] = i + 1; 
      compt = compt + 1;
      return (compt);
    }
}
