void	 my_putstr(char *str);
void	 my_putchar(char c);
char*	 my_strchr(char *str, char c)
{
  while (*str != '\0')
    {
      if (*str != c)
	{
	  str++;
	}
      else if (*str == c)
	{
	  return (str);
	}
    }
}
