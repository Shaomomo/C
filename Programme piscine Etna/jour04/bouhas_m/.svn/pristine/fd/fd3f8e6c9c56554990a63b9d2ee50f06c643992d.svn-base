#include <unistd.h>
void my_putchar(char c)
{
  write(1, &c, 1);
}
char *my_strcat(char *dest, char *src);
{
  int	i;
  int	j;
  char *tempo;
    
  i = 0;
  j = 0;
  while(src[i] != '\0')
    {
      j = j + 1;
    }
  while(dest[j] != '\0')
    {
      src[i] = dest[j];
      j = j + 1;
      i = i + 1;
    }
  return (src[i])
}

int main(void)
{
  char dest[] = "coucou";
  char src[] = "copain";
  my_putchar(my_strcat(dest, src));
}
